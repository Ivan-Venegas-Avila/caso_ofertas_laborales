<p id="notice"><%= notice %></p>

<h1>Postulaciones</h1>

<table>
  <thead>
    <tr>
      <th>Postulante</th>
      <th>Empresa</th>
      <th>Oferta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postulaciones.each do |postulacion| %>
      <tr>
        <td><%= postulacion.postulante_id %></td>
        <td><%= postulacion.empresa_id %></td>
        <td><%= postulacion.oferta_id %></td>
        <td><%= link_to 'Show', postulacion %></td>
        <td><%= link_to 'Edit', edit_postulacion_path(postulacion) %></td>
        <td><%= link_to 'Destroy', postulacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Postulacion', new_postulacion_path %>
