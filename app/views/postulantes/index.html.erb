<p id="notice"><%= notice %></p>

<div class="container">

    <h1>Postulantes</h1>

    <table class=table-bordered>
      <thead>
        <tr>
          <th>Rut</th>
          <th>Nombre</th>
          <th>Fecha nacimiento</th>
          <th>Genero</th>
          <th>Telefono</th>
          <th>Curriculum</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @postulantes.each do |postulante| %>
          <tr>
            <td><%= postulante.rut %></td>
            <td><%= postulante.nombre %></td>
            <td><%= postulante.fecha_nacimiento %></td>
            <td><%= postulante.genero %></td>
            <td><%= postulante.telefono %></td>
            <td><%= postulante.curriculum %></td>
            <td><%= link_to 'Show', postulante %></td>
            <td><%= link_to 'Edit', edit_postulante_path(postulante) %></td>
            <td><%= link_to 'Destroy', postulante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Postulante', new_postulante_path %>
</div>
